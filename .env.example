# Google Maps API Key (for geocoding and nearby search)
# Get your key from: https://console.cloud.google.com/google/maps-apis
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Mapbox Access Token (alternative to Google Maps)
# Get your token from: https://account.mapbox.com/access-tokens/
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

# Foursquare API Key (for places search and recommendations)
# Get your key from: https://developer.foursquare.com/
VITE_FOURSQUARE_API_KEY=your_foursquare_api_key_here
VITE_FOURSQUARE_BASE_URL=https://api.foursquare.com/v3/places

# OpenWeatherMap API Key (for weather data)
# Get your key from: https://openweathermap.org/api
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Currency API Key (optional, for currency conversion)
# Get your key from: https://exchangerate-api.com/ or https://fixer.io/
VITE_CURRENCY_API_KEY=your_currency_api_key_here
